{
	"compilerOptions": {
		"moduleResolution": "Node",
		"target": "ESNext",
		"module": "ESNext",
		"sourceMap": true,
		"declaration": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true,
		"strictBindCallApply": false,
		"baseUrl": "."
	}
}
